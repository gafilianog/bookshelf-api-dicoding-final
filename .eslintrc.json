{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "max-len": ["error", { "code": 120 }],
    "no-console": "off"
  }
}
